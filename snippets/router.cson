'.source.coffee':
  'Nested resource':
    'prefix': '@resource-v'
    'body':
      """
        @resource( '${1:name}' , path: '${2:/some/item_id}' ) , ->
          @route "route_name"
      """

  'verbose route':
    'prefix': '@route-v'
    'body':  "@route('${1:name}', path:'${2:/some/item_id}')"

  'verbose @route':
    'prefix': '@route-v'
    'body':  "@route('${1:name}', path:'${2:/some/item_id}')"

