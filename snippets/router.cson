'.source.coffee':
  'Nested resource':
    'prefix': 'em_router_@resource'
    'body':
      """
        @resource( '${1:name}' , path: '${2:/some/item_id}' ) , ->
          @route "route_name"
      """
      
  'verbose route':
    'prefix': 'em_router_@route-v'
    'body':  "@route('${1:name}', path:'${2:/some/item_id}')"
    
    
  'verbose @route':
    'prefix': '@route-v'
    'body':  "@route('${1:name}', path:'${2:/some/item_id}')"
